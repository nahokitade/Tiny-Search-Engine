	$(function () {
		//never send if in iframe. must use try{} to prevent security exception when in cross-domain frame
		try {
			if (window != window.top)
				return;
		} catch (err) { }

		//track viewport dimensions
		ga_trackDimensions('Initial');

		//track viewport dimensions being changed by resize (throttled)
		var gaResizeCompleteHl;
		$(window).resize(function () {
			clearTimeout(gaResizeCompleteHl);
			gaResizeCompleteHl = setTimeout(function () {
				ga_trackDimensions('Resized');
			}, 500);
		});
	});

	function ga_trackDimensions(typeLabel) {
		var viewportWidth = $(window).width();
		var viewportHeight = $(window).height();
		_gaq.push(
      ['_trackEvent', 'Viewport Dimensions', 'Viewport Width ' + typeLabel, viewportWidth + 'x' + viewportHeight, viewportWidth, true],
      ['_trackEvent', 'Viewport Dimensions', 'Viewport Height ' + typeLabel, viewportWidth + 'x' + viewportHeight, viewportHeight, true]
     );
	}
