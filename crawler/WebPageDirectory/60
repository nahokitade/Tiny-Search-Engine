<script type="text/javascript">

	
	$(document).ready(function() {

		


		downloadPlayerPopup();
			//savethecurrentdemonstrationauthor();
			
			
			
			

		if($('.searchboxsub').attr('value') != 'SEARCH') {
					$('.searchboxsub').css('color','black');
				}
				$('.searchboxsub').focus(function() { 
					if($('.searchboxsub').attr('value') == 'SEARCH') {
						$('.searchboxsub').attr('value','').css('color','black');
					}
				});
				$('.searchboxform').submit(function(){ 
					if($('.searchboxsub').attr('value') == 'SEARCH') {
						return false;
					}
				});
				$('.searchboxsub').blur(function() {
					if(!$('.searchboxsub').attr('value')) {
						$('.searchboxsub').attr('value','SEARCH').css('color','grey');
					}
				});
				if($(".header h1").length>0){
					document.title = "Wolfram Demonstrations Project: "+ $(".header h1").html();
				}
	});

setTimeout(function(){ 
                if($('.demoflash').width() <= 660) {
			$('.demoflash').width(660); // Must need for IE 7
                        //$('.pageWidth').width($('.demoflash').width() + 375);
                }
}, 1000);

			function checkform(form){
				if(form.query.value != "SEARCH")
				{
					form.query.value = jQuery.trim(form.query.value);
					var newquery = encodeURIComponent(form.query.value);
					var ccssRegex = /^CCSS.Math.\S+$/i;
					if(ccssRegex.test(newquery)) {
				       var newurl = "/education.html?edutag="+newquery;
					} else {
					   var newurl = "/search.html?query="+newquery;
					}
					var url = Utf8.encode(newurl);
					window.location = url;
				}
				return false;
		   	}

</script>
