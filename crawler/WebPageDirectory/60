<script type="text/javascript" src="/javascript/json-1.1.min.js"></script>
<script type="text/javascript" src="/javascript/jquery.simplemodal-1.4.1.js"></script>
<script type="text/javascript" src="/javascript/css_browser_selector.js"></script>
<script type="text/javascript">
$(document).ready(function() {
	$('.newfeedback').click(function (e) {
		$('#new-feedback-content').modal();
		return false;
	});
	
	$('#sendfeedbackbutton').click(function (e) {
		if(!validateForm()){
			return;
		}
		var dataString = 'feedback='+ $('#feedback').val() + 
				   '&sourcepage=' + window.location.toString() + 
     				   '&email=' + $('#feedback-email').val() + 
				   '&name=' + $('#feedback-name').val() + 
				   '&occupation=' + $('#feedback-occupation').val() + 
				   '&organization=' + $('#feedback-organization').val() +
 				   '&oft=' + '' + 
				   '&country=' + '' +
				   '&remme=';

		$.ajax({
			type: 'post',
 			url: "/feedback/feedback.html",
			data: dataString,
  			success: function(data){
				changeInnerHtml();
    			//alert(data);
				setTimeout("$.modal.close()",4000);
  			},
			error: function(xhr, desc, err) {
        		}
		});
	});

	if($('#feedback-name').attr('value') != 'optional') {
             $('#feedback-name').css('color','black').css('font-style','normal');
       }  
       $('#feedback-name').focus(function() {
             if($('#feedback-name').attr('value') == 'optional') {
                   $('#feedback-name').attr('value','').css('color','black').css('font-style','normal');
             }
       });
       $('#feedback-name').blur(function() {
             if(!$('#feedback-name').attr('value')) {
                    $('#feedback-name').attr('value','optional').css('color','grey').css('font-style','italic');
             }
       });
	if($('#feedback-occupation').attr('value') != 'optional') {
             $('#feedback-occupation').css('color','black').css('font-style','normal');
       }  
       $('#feedback-occupation').focus(function() {
             if($('#feedback-occupation').attr('value') == 'optional') {
                   $('#feedback-occupation').attr('value','').css('color','black').css('font-style','normal');
             }
       });
       $('#feedback-occupation').blur(function() {
             if(!$('#feedback-occupation').attr('value')) {
                    $('#feedback-occupation').attr('value','optional').css('color','grey').css('font-style','italic');
             }
       });
	if($('#feedback-organization').attr('value') != 'optional') {
             $('#feedback-organization').css('color','black').css('font-style','normal');
       }  
       $('#feedback-organization').focus(function() {
             if($('#feedback-organization').attr('value') == 'optional') {
                   $('#feedback-organization').attr('value','').css('color','black').css('font-style','normal');
             }
       });
       $('#feedback-organization').blur(function() {
             if(!$('#feedback-organization').attr('value')) {
                    $('#feedback-organization').attr('value','optional').css('color','grey').css('font-style','italic');
             }
       });	
});

function changeInnerHtml()
{
	$('#new-feedback-content').html("");
	var newHTML = "<div class='successfulMsg1'>Thanks for sending us a message!<div class='successfulMsg2'>We read all comments and appreciate your input.</div></div>";
	$('#new-feedback-content').html(newHTML);
}

function validateForm(){
	var error = "";
	$('#feedback').css("background-color", "#FFF");
	$('#feedback-email').css("background-color", "#FFF");
	document.getElementById("feedbackError").innerHTML = "";
	if($.browser.msie){
		$('#simplemodal-container').height(300);
	}
	else{
		$('#simplemodal-container').height(290);
	}
  	if($('#feedback').val() == ""){
    		error = error+"Please leave your feedback.<br>";
		$('#feedback').css("background-color", "#FFEFEF");
	}

  	if(window.location.toString() == ""){
    		error = error+"There was an error in the form submission, please ensure that JavaScript is enabled.<br>";
	}
	
	var regex = new RegExp('^(.*)@(.*)\.[a-zA-Z]+$');
	var emailvalue = $('#feedback-email').val();
	if(!emailvalue.match(regex)){
		error = error+"Please give a valid email address.<br>";
		$('#feedback-email').css("background-color", "#FFEFEF");
	}
	
	if(error.length > 0){
		document.getElementById("feedbackError").innerHTML = error;
		if($.browser.msie){
			$('#simplemodal-container').height(320);
		}
		else{
			$('#simplemodal-container').height(310);
		}

		return false;
	} else {
		return true;
	}
}

</script>
