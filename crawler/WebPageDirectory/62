<script>
  //<![CDATA[
    function update_example_rows(){
      var offset = parseInt($('#header_lines').val()) || 0;
      var example_count = Math.min(6, travelling_geocoding.uploaded_data.length - offset) + offset;
      var cols = travelling_geocoding.uploaded_data[0].length;
      var out = '';
      for (var i = offset; i < example_count; i++){
        out += '<tr>';
        for (var y = 0; y < cols; y++){
          if (travelling_geocoding.uploaded_data[i][y] == null){
            out += '<td>-</td>';
          } else {
            out += '<td>' + travelling_geocoding.uploaded_data[i][y] + '</td>';
          }
        }
        out += '</tr>';
      };
      $('.upload_table table tbody tr').remove();
      $('.upload_table table tbody').append(out);
      $('.upload_table table tbody tr td').css("vertical-align", "top");
      $('.upload_table table tbody tr:odd').css("background-color", "rgb(250, 250, 250)");
    };
    
    
    function zuordnung_isvalid(zuordnung){
      var min_required_combi = [
        ['latitude', 'longitude'],
        ['str', 'ort'],
        ['str', 'plz'],
        ['str', 'plz_ort'],
        ['str_hnr', 'ort'],
        ['str_hnr', 'plz'],
        ['str_hnr', 'plz_ort']
      ];
      var valid = 'invalid_combi';
      // check, ob gültige kombinationen ausgewählt wurden
      $.each(min_required_combi, function(i, v){
        var c1 = 0;
        var c2 = v.length;
        $.each(zuordnung, function(i_i, v_v){
          if ($.inArray(v_v[1], v) > -1){
            c1++;
          };
        });
        if (c1 == c2){
          valid = v;
          return false; // means break :-(
        };
      });
      // check, ob ein feld mehrfach ausgewählt wurde
      var test_array = [];
      $.each(zuordnung, function(i,e){ 
        if($.inArray(e[1], test_array) === -1) test_array.push(e[1]) ;
      });
      if (test_array.length != zuordnung.length){
        valid = 'multiple';
      }
      return valid;
    };
    
    jQuery.fn.center = function () {
      this.css("position","absolute");
      this.css("top", ( jQuery(window).height() - this.height() ) / 2+jQuery(window).scrollTop() + "px");
      this.css("left", ( jQuery(window).width() - this.width() ) / 2+jQuery(window).scrollLeft() + "px");
      return this;
    };
    
    $('#close_icon').click(
      function () {
        // change other exit too - search for dfe6iWTjxOgeY
        $('#alert_popup')
        .removeClass('alert_popup_select_fields')
        .css("position", "")
        .css("top", "")
        .css("left", "")
        .hide();
        $('#alert_screen').hide();
      }
    );
    
    $(document).ready(function() {
    
      $('#alert_popup').off('click', '#upload_start_geocode').on('click', '#upload_start_geocode', function(e){
        var offset = parseInt($('#header_lines').val()) || 0;
        var zuordnung = [];
        $('#alert_message .upload_table .upload_select_fields').each(function(i){
          if ($(this).val() != '-'){
            zuordnung.push([$(this).attr('data-id'), $(this).val()]);
          }
        });
        var selected_zuordnung = zuordnung_isvalid(zuordnung);
        switch (selected_zuordnung){
          case 'invalid_combi':
            $('#upload_error').html(texte.feldzuordnung_fehler_keine_kombi);
          break;
          case 'multiple':
            $('#upload_error').html(texte.feldzuordnung_fehler_mehrfach_zuordnung);
          break;
          default:
            // change other exit too - search for dfe6iWTjxOgeY
            $('#alert_popup').removeClass('alert_popup_select_fields').css("position", "").css("top", "").css("left", "");
            $('#upload_error').empty();
            travelling_geocoding.work_from_import(zuordnung, offset, selected_zuordnung);
        };
      });
    
      var iscountryCodeClicked = false;
    
      $('select.target_country').click(function() {
        $.cookie("gbc_usercountry", $('select.target_country').val());
        iscountryCodeClicked = true;
      });
    
      if ($.cookie("gbc_usercountry") == null) {
        $.getJSON('//api.wipmania.com/jsonp?callback=?', '', function(json) {
          if (!iscountryCodeClicked) {
            var countryCode = json.address.country_code.toLowerCase();
            $('select.target_country option[value=' + countryCode + ']').attr('selected', 'selected');
            $.cookie("gbc_usercountry", countryCode);
          }
        });
      } else {
        var countryCode = $.cookie("gbc_usercountry");
        $('select.target_country option[value=' + countryCode + ']').attr('selected', 'selected');
      }
      
      $('#upload_form').ajaxForm({
        beforeSend: function() {
        },
        uploadProgress: function(event, position, total, percentComplete) {
        },
        success: function(text) {
          var data = null;
          if (typeof text === "object") {data = text;}
          else {
            // IE 9.0 und niedriger
            try {
              data = $.parseJSON(text);
            }
            catch (e) {
              data = {errormessage : texte.upload_can_not_open_file};
            }
          }
    
          if (data && data.errormessage){
            showAlert(data.errormessage);
          } else {
            travelling_geocoding.uploaded_data = data.values;
            var cols = data.values[0].length;
            var out = '<label for="header_lines">' + texte.upload_header_lines + '</label>&nbsp;<input id="header_lines" max="99" min="0" name="header_lines" type="number"><br /><br />';
            out += '<div class="upload_table"><table style="width:100%;"><thead><tr>';
            for (var i = 0; i < cols; i++){
              out += '<td>' + $('#upload_select_fields').html() + '</td>';
            }
            out += '</tr></thead><tbody>';
            out += '</tbody></table></div>';
            out += '<div id="upload_error"></div>';
            out += '<div class="upload_start_geocode_div"><button class="button" id="upload_start_geocode">' + texte.feldzuordnung_start_geocoding + '</button></div>';
            $('#alert_message').html(out);
            
            var offset = parseInt(data.upload_offset) || 0;
            $('#header_lines').val(offset);
            update_example_rows();
            
            $('#alert_popup_title').html(texte.feldzuordnung + ' "' + data.file + '" (' + data.values.length + ' ' + texte.upload_counted_rows + ')');
            $('#alert_popup')
            .addClass('alert_popup_select_fields')
            .show()
            .center();
            
            $('#header_lines').off('change').on('change', function(){
              update_example_rows();
            });
            
            $('.upload_select_fields').each(function(i){
              $(this).attr('data-id', i);
            });
            if (data.zuordnungen){
              $.each(data.zuordnungen, function (i, v){
                $('.upload_select_fields').eq(v[0]).val(v[1]);
              });
            };
            $('.upload_select_fields').on('change', function(e){
              $('#upload_error').empty();
            });
            $('#alert_screen').show();
          }
        },
        complete: function(xhr) {
        }
      });
      if ($('#tsp_first_col_is_desc').is(':checked')) {
        $('#delimiter_dsc').show();
      }
    });
  //]]>
</script>

</div>
